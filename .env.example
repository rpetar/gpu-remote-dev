# Environment variables template for container deployment
# Copy this to .env and fill in your values (DO NOT commit .env to git!)

# Azure Storage Account credentials
ACCOUNT_NAME=your-azure-storage-account-name
ACCOUNT_KEY=your-azure-storage-account-key

# VS Code Tunnel configuration
TUNNEL_ID=my-gpu-dev-tunnel
TUNNEL_NAME=my-gpu-dev-tunnel-name
ACCESS_TOKEN=your-github-access-token-here

# Optional: Azure blob container name (default is 'workspace')
CONTAINER_NAME=workspace


GITHUB_REPO_URL=https://github.com/user/repo.git
GITHUB_TOKEN=ghp_xxxxxxxxxxxxxxxxxxxxx
PROJECT_DIR=/mnt/workspace/project
CLONE_ON_START=true